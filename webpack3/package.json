{
  "name": "vue-static",
  "version": "1.0.0",
  "description": "learn vue static",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:practice": "rimraf dist/practice && cross-env NODE_ENV=production webpack --config build/webpack.config.practice.js",
