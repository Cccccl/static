    "rimraf": "^2.6.2",
    "url-loader": "^1.1.2",
    "vue-loader": "14.0.0",
    "vue-template-compiler": "^2.0.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
