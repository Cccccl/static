    "build:vueExpand": "rimraf dist/repertory && cross-env NODE_ENV=production webpack --config build/webpack.config.vueExpand.js",
    "dev:vueExpand": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.vueExpand.js",
    "lint": "eslint --ext .js,.jsx,.vue practice repertory",
    "lint-fix": "eslint --fix --ext .js,.jsx,.vue practice repertory"
  },
  "keywords": [
    "vue"
  ],
  "author": "ccc1l",
  "license": "ISC",
